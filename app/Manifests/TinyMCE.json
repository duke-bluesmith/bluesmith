{
	"source": "tinymce/tinymce",
	"destination": "vendor/tinymce",
	"resources": [
		{
			"source": ".",
			"filter": "#\\.js$#"
		},
		{
			"source": "plugins",
			"destination": "plugins",
			"recursive": true
		},
		{
